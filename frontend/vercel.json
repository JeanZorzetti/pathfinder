{
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "https://pathback.roilabs.com.br/api/v1/content/sitemap.xml"
    },
    {
      "source": "/api/:path*",
      "destination": "https://pathback.roilabs.com.br/api/:path*"
    }
  ],
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, s-maxage=3600"
        }
      ]
    }
  ]
}
